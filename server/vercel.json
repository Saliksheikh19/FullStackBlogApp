{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node-server"
      }
    ],
    "routes": [
      {
        "src": "/posts/(.*)",
        "dest": "/posts/$1"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/auth/$1"
      },
      {
        "src": "/user/(.*)",
        "dest": "/user/$1"
      },
      {
        "src": "/category/(.*)",
        "dest": "/category/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  